#ifndef UART_COMMUNICATION_CODES_H_
#define UART_COMMUNICATION_CODES_H_


#define UART_CONFIRMATION 0xFF
#define UART_NEW_PASSWORD 0xA1
#define UART_VALID_PASSWORDS 0xA2
#define UART_INVALID_PASSWORDS 0xA0
#define UART_LOGIN 0X02
#define UART_LOGIN_SUCCESS 0X03
#define UART_LOGIN_FAIL 0X04





#endif /* UART_COMMUNICATION_CODES_H_ */
